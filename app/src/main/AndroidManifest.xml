<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sweetbetty.roomDiabetesEntries">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.sweetbetty.roomDiabetesEntries.MainActivityNeuNeuNeu"
            android:configChanges="orientation"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/duestertheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sweetbetty.roomDiabetesEntries.EintragInFirestore" />
        <activity android:name="com.sweetbetty.roomDiabetesEntries.UndoDeleteNote" />
        <activity
            android:name="com.sweetbetty.roomDiabetesEntries.AddEditNoteActivity"
            android:configChanges="orientation"
            android:parentActivityName="com.sweetbetty.roomDiabetesEntries.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/duesterAddEdittheme" />
        <activity android:name="com.sweetbetty.roomDiabetesEntries.EinstellungenTheme"></activity>



    </application>

</manifest>